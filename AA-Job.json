{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accountName": {
            "type": "string",
            "defaultValue": "ARM-Account"
        },
        "runbookName": {
            "type": "string",
            "defaultValue": "ARM-Runbook"
        },
        "jobId": {
            "type": "string",
            "defaultValue": "6a805987-ff91-4f18-99ef-2e5686592a16"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "Account-and-Runbook",
            "resourceGroup": "ARM-TEST",
            "dependsOn": [],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/hjensen8/AAJob/master/AA-Account-and-Runbook.json"
                },
                "parameters": {
                    "accountName": {
                        "value": "[parameters('accountName')]"
                    },
                    "runbookName": {
                        "value": "[parameters('runbookName')]"
                    }
                }
            }
        },
        {
            "name": "[parameters('jobId')]",
            "type": "jobs",
            "apiVersion": "2015-01-01-preview",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[concat('Microsoft.Automation/automationAccounts/', parameters('accountName'))]",
                "[concat('Microsoft.Automation/automationAccounts/', parameters('accountName'), '/runbooks/',parameters('runbookName'))]"
            ],
            "tags": {
                "key": "value"
            },
            "properties": {
                "runbook": {
                    "name": "[parameters('runbookName')]"
                }
            }
        }
    ]
}